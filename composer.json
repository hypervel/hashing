{
    "name": "swooletw/hyperf-hashing",
    "type": "library",
    "description": "The hashing package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "hashing",
        "swoole",
        "laravel-hyperf"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Hashing\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "hyperf/config": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/support": "~3.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Hashing\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    }
}